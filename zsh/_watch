#compdef watch

# Generated with perl module App::Spec v0.013

_watch() {
    local program=watch
    typeset -A opt_args
    local curcontext="$curcontext" state line context


        # ---- Command: 
        _arguments -s  \
            '1: :->command' \
            '--version[output version information and exit]' \
            '-v[output version information and exit]' \
            '--beep[beep if command has a non-zero exit]' \
            '-b[beep if command has a non-zero exit]' \
            '--color[interpret ANSI color and style sequences]' \
            '-c[interpret ANSI color and style sequences]' \
            '--errexit[exit if command has a non-zero exit]' \
            '--chgexit[exit when output from command changes]' \
            '--interval[seconds to wait between updates]:interval' \
            '-n[seconds to wait between updates]:interval' \
            '--precise[attempt run command in precise intervals]' \
            '-p[attempt run command in precise intervals]' \
            '--no-title[turn off header]' \
            '-t[turn off header]' \
            '--exec[pass command to exec instead of "sh -c"]:exec' \
            '-x[pass command to exec instead of "sh -c"]:exec' \
            '--differences[highlight changes between updates]:differences:("cumulative")' \
            '-d[highlight changes between updates]:differences:("cumulative")' \
            '--help[Show command help]' \
            '-h[Show command help]' \
            && ret=0

        case $state in
        command)

        ;;
        esac


}


__watch_dynamic_comp() {
    local argname="$1"
    local arg="$2"
    local comp="arg:$argname:(("
    local line
    while read -r line; do
        local name="$line"
        local desc="$line"
        name="${name%$'\t'*}"
        desc="${desc/*$'\t'}"
        comp="$comp$name"
        if [[ -n "$desc" && "$name" != "$desc" ]]; then
            comp="$comp\\:"'"'"$desc"'"'
        fi
        comp="$comp "
    done <<< "$arg"

    comp="$comp))"
    _alternative "$comp"
}

